#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/vitest@3.1.1_@types+node@22.13.10_jiti@2.4.2_jsdom@20.0.3_msw@2.10.2_@types+node@22.13._25196b7cdefba129cc422f1d1275fe05/node_modules/vitest/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/vitest@3.1.1_@types+node@22.13.10_jiti@2.4.2_jsdom@20.0.3_msw@2.10.2_@types+node@22.13._25196b7cdefba129cc422f1d1275fe05/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/vitest@3.1.1_@types+node@22.13.10_jiti@2.4.2_jsdom@20.0.3_msw@2.10.2_@types+node@22.13._25196b7cdefba129cc422f1d1275fe05/node_modules/vitest/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/vitest@3.1.1_@types+node@22.13.10_jiti@2.4.2_jsdom@20.0.3_msw@2.10.2_@types+node@22.13._25196b7cdefba129cc422f1d1275fe05/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
