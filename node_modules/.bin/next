#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/next@15.2.6_@babel+core@7.26.10_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-_f582bd5de6e8c5c619384e99b96ecbf1/node_modules/next/dist/bin/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/next@15.2.6_@babel+core@7.26.10_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-_f582bd5de6e8c5c619384e99b96ecbf1/node_modules/next/dist/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/next@15.2.6_@babel+core@7.26.10_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-_f582bd5de6e8c5c619384e99b96ecbf1/node_modules/next/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/next@15.2.6_@babel+core@7.26.10_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-_f582bd5de6e8c5c619384e99b96ecbf1/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/next@15.2.6_@babel+core@7.26.10_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-_f582bd5de6e8c5c619384e99b96ecbf1/node_modules/next/dist/bin/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/next@15.2.6_@babel+core@7.26.10_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-_f582bd5de6e8c5c619384e99b96ecbf1/node_modules/next/dist/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/next@15.2.6_@babel+core@7.26.10_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-_f582bd5de6e8c5c619384e99b96ecbf1/node_modules/next/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/next@15.2.6_@babel+core@7.26.10_@opentelemetry+api@1.9.0_@playwright+test@1.52.0_react-_f582bd5de6e8c5c619384e99b96ecbf1/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
