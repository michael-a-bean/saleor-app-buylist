#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/msw@2.10.2_@types+node@22.13.10_typescript@5.8.2/node_modules/msw/cli/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/msw@2.10.2_@types+node@22.13.10_typescript@5.8.2/node_modules/msw/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/msw@2.10.2_@types+node@22.13.10_typescript@5.8.2/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/msw@2.10.2_@types+node@22.13.10_typescript@5.8.2/node_modules/msw/cli/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/msw@2.10.2_@types+node@22.13.10_typescript@5.8.2/node_modules/msw/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/msw@2.10.2_@types+node@22.13.10_typescript@5.8.2/node_modules:/home/michael/saleor-platform/saleor-apps/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/msw@2.10.2_@types+node@22.13.10_typescript@5.8.2/node_modules/msw/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/msw@2.10.2_@types+node@22.13.10_typescript@5.8.2/node_modules/msw/cli/index.js" "$@"
fi
